    private void studentActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
    }                                       

    private void clearActionPerformed(java.awt.event.ActionEvent evt) {                                      
          model.setRowCount(0);
    }                                     

    private void addActionPerformed(java.awt.event.ActionEvent evt) {                                    
        String ids = id.getText();
        String name = student.getText();
        String age = Age.getText();
        model.addRow(new Object[]{name, ids, age, course.getSelectedItem()});
        
        Age.setText("");
        id.setText("");
        student.setText("");
        student.requestFocus();
        
// TODO add your handling code here:
    }                                   

    private void AgeActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
    }                                   

